{
  "name": "cborjs-typedarray-decoder",
  "description": "Decodes typed arrays from CBOR",
  "version": "0.1.0",
  "main": "decoder.js",
  "scripts": {
    "pretest": "standard",
    "test": "mocha",
    "prebuild": "npm test",
    "build": "browserify decoder.js -s CBORTypedArrayDecoder -o cborjs-typedarray-decoder.js",
    "postbuild": "uglifyjs cborjs-typedarray-decoder.js -c unused=false -o cborjs-typedarray-decoder.min.js --source-map cborjs-typedarray-decoder.min.js.map",
    "prepublish": "npm test"
  },
  "standard": {
    "ignore": [
      "test/"
    ]
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "mocha": "^2.3.0",
    "standard": "^5.1.1",
    "uglify-js": "^2.4.24"
  }
}
